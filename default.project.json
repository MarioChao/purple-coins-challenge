{
  "name": "purple-coins-challenge",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "Modules": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true,

        "PurpleCoins": {
          "$path": "src/ReplicatedStorage/PurpleCoins"
        }
      },

      "RemoteEvents": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true,

        "PurpleCoinsRemoteEvent": {
          "$className": "RemoteEvent"
        }
      }
    },

    "ServerScriptService": {
      "Modules": {
        "$path": "ServerPackages",
        "$ignoreUnknownInstances": true
      },

      "PurpleCoins": {
        "$className": "Folder",

        "ChallengesStorage": {
          "$path": "src/ServerScriptService/PurpleCoins/ChallengesStorage",
          "$ignoreUnknownInstances": true
        },

        "GetChallenges": {
          "$path": "src/ServerScriptService/PurpleCoins/GetChallenges"
        },

        "PurpleCoinsMain": {
          "$path": "src/ServerScriptService/PurpleCoins/PurpleCoinsMain"
        },

        "_Constants": {
          "$path": "src/ServerScriptService/PurpleCoins/_Constants"
        }
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "$path": "src/StarterPlayerScripts",
        "$ignoreUnknownInstances": true
      }
    },

    "Workspace": {
      "_Workspace": {
        "$className": "Folder",

        "CoinsFolder": {
          "$className": "Folder"
        }
      }
    }
  },
  "globIgnorePaths": ["**/wally.toml"]
}